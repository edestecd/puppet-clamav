<%= ERB.new(File.read(File.expand_path("_header.erb",File.dirname(file)))).result(binding) -%>
<%- if @sort_options -%>
  <%- co = @config_options.sort -%>
<%- else -%>
  <%- co = @config_options -%>
<%- end -%>

<% co.each do |k,v| -%>
<% Array(v).each do |av| -%>
<% if ![nil, '', :undef].include?(av) -%>
<%= %Q(#{k} #{av}) %>
<% end -%>
<% end -%>
<% end -%>
